
# Settings

record(bo, "$(P)SIMBON_CMD") {
  field(ZNAM, "Off")
  field(ONAM, "Beam On")
  field(OUT , "$(P)Cnf_CSET_.B3 PP")
  field(VAL , "0")
  field(PINI, "YES")
  info(autosaveFields_pass0, "VAL")
}

