

record(bo, "$(SYS)-$(EVR):$(OBJ)-Update-Cmd") {
  field(DTYP, "Obj Prop bool")
  field(SCAN, "10 second")
  field(OUT , "@OBJ=$(EVR):$(OBJ), PROP=Update")
  field(ZNAM, "Update")
  field(ONAM, "Update")
  field(FLNK, "$(SYS)-$(EVR):$(OBJ)-T-I")
}

