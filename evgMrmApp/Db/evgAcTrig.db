record(longout, "$(P):acTrig:divider") {
    field( DTYP, "EVG AC Divider")
    field( DESC, "EVG AC Divider")
    field( OUT,  "#C$(cardNum) S @")
    field( PINI, "YES")
    field( VAL,  "0")
    field( UDF,  "0")
    field( HIHI, "256")
    field( LOLO, "-1")
    field( HHSV, "MAJOR")
    field( LLSV, "MAJOR")
    info( autosaveFields_pass0, "VAL")
}

record(ao, "$(P):acTrig:phase") {
    field( DTYP, "EVG AC Phase")
    field( DESC, "EVG AC Phase Shifter")
    field( OUT,  "#C$(cardNum) S @")
    field( PINI, "YES")
    field( VAL,  "0.0")
    field( UDF,  "0")
    field( HIHI, "25.6")
    field( LOLO, "-0.1")
    field( HHSV, "MAJOR")
    field( LLSV, "MAJOR")
    field( EGU,  "msec")
    field( HOPR, "25.5")
    field( LOPR, "0")
    field( DRVH, "25.5")
    field( DRVL, "0")
    field( PREC, "1")
    info( autosaveFields_pass0, "VAL")
}

record(bo, "$(P):acTrig:byp") {
    field( DTYP, "EVG AC Bypass")
