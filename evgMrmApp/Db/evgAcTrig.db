record(longout, "$(P)Divider-SP") {
  field(DESC, "EVG AC Divider")
  field(ASG,  "$(ASGPROTECTED=protected)")
  field(DTYP, "Obj Prop uint32")
  field(OUT,  "@OBJ=$(OBJ), PROP=Divider")
  field(PINI, "YES")
  field(VAL,  "0")
  field(UDF,  "0")
  field(HIHI, "256")
  field(LOLO, "-1")
  field(HHSV, "MAJOR")
  field(LLSV, "MAJOR")
  field(FLNK, "$(P)Divider-RB")
