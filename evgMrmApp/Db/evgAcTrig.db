    field( VAL , "1")
    field( UDF,  "0")
    field( ZNAM, "Off")
    field( ONAM, "On")
    info( autosaveFields_pass0, "VAL")
}

record(bo, "$(P):acTrig:syncSrc") {
    field( DTYP, "EVG AC Sync")
