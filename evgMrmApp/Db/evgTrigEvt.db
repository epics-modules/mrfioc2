record(bo, "$(P):$(N):Ena") {
	field( DTYP, "EVG Trig Evt")
	field( DESC, "EVG Trigger Event Enable")
	field( OUT , "#C$(cardNum) S$(trigEvtNum) @")
	field( ZNAM, "Disabled")
	field( ONAM, "Enabled")
	field( PINI, "YES")
	field( VAL, "1")
	field( UDF, "0")
}

record(longout, "$(P):$(N):Code") {
	field( DTYP, "EVG Trig Evt")
	field( DESC, "EVG Trigger Event Code")
	field( OUT, "#C$(cardNum) S$(trigEvtNum) @")
	field( PINI, "YES")
	field( VAL, "0")
	field( UDF, "0")
	field(HIHI, "256")
	field(LOLO, "-1")
	field(HHSV, "MAJOR")
	field(LLSV, "MAJOR")
	info(autosaveFields_pass0, "VAL")
}

