# Too many possible sources for one mbbo record, so use several
# Each mbbo has an associated seq record with the following steps:
# 1. Set SrcChanging, indicates a selection is being processed
#    and prevents other seq's interfering using SDIS.
# 2. Save desired selection value.
# 3-5. Set all the mbbo records to 'Off' (0)
# 6-7. Process the desired selection.
# 10. Reset SrcChanging.
record(mbbo, "$(P)Src:FP-Sel_") {
    field( DTYP, "Raw Soft Channel")
    field( UDF,  "0")
    field( ZRST, "Off")
    field( ONST, "AC")
    field( TWST, "Front0")
    field( THST, "Front1")
    field( FRST, "Front2")
    field( ZRVL, "0x0")
    field( ONVL, "0x1")
    field( TWVL, "0x2")
    field( THVL, "0x4")
    field( FRVL, "0x8")
    field( IVOA, "Don't drive outputs")
    field( FLNK, "$(P)Src:FP-Select_")
    info( autosaveFields_pass0, "VAL")
    alias("$(P)Src0-Sel")
}

record(mbbo, "$(P)Src:Mxc-Sel_") {
    field( DTYP, "Raw Soft Channel")
    field( UDF,  "0")
    field( ZRST, "Off")
    field( ONST, "Mxc0")
    field( TWST, "Mxc1")
    field( THST, "Mxc2")
    field( FRST, "Mxc3")
    field( FVST, "Mxc4")
    field( SXST, "Mxc5")
    field( SVST, "Mxc6")
    field( EIST, "Mxc7")
    field( ZRVL, "0x0")
    field( ONVL, "0x1")
    field( TWVL, "0x2")
    field( THVL, "0x4")
    field( FRVL, "0x8")
    field( FVVL, "0x10")
    field( SXVL, "0x20")
    field( SVVL, "0x40")
    field( EIVL, "0x80")
    field( IVOA, "Don't drive outputs")
