record(seq, "$(P)Src:FP-Select_") {
    field( DESC, "Sequence to control Src Select")
    field( SDIS, "$(P)SrcChanging_.VAL NPP NMS")
    field( SELM, "All")
    field( DOL1, "1")
    field( LNK1, "$(P)SrcChanging_.VAL PP NMS")
    field( DOL2, "$(P)Src:FP-Sel_.VAL NPP NMS")
    field( LNK2, "$(P)Src:FP-Select_.DO6 NPP NMS")
    field( DOL3, "0")
    field( LNK3, "$(P)Src:Mxc-Sel_.VAL PP NMS")
    field( DOL4, "0")
    field( LNK4, "$(P)Src:Univ-Sel_.VAL PP NMS")
    field( LNK5, "$(P)SrcReset0_.PROC PP NMS")
    field( DOL6, "$(P)Src:FP-Select_.DO6 NPP NMS")
