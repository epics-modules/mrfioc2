# By default OMSL field for the *Src* records is "closed_loop" hence
# only one Source can be selected due of MBBO record. If you want to select
# multiple sources then the OMSL field for the *Src* records should be
# "supervisory" mode. In order to select multiple sources use directly *Src*
# records bypassing the Src-Sel record.
#
record(dfanout, "$(P)Omsl-FOut") {
    field( PINI, "YES")
    field( VAL,  "1")
    field( UDF,  "0")
    field( OMSL, "supervisory")
    field( OUTA, "$(P)Src:AC-Sel.OMSL")
    field( OUTB, "$(P)Src:FrontInp0-Sel.OMSL")
    field( OUTC, "$(P)Src:FrontInp1-Sel.OMSL")
    field( OUTD, "$(P)Src:FrontInp2-Sel.OMSL")
    field( OUTE, "$(P)Src:Mxc2-Sel.OMSL")
    field( OUTF, "$(P)Src:Mxc3-Sel.OMSL")
    field( OUTG, "$(P)Src:Mxc4-Sel.OMSL")
    field( OUTH, "$(P)Src:Mxc5-Sel.OMSL")
    field( FLNK, "$(P)Omsl:Cont-FOut0_.PROC")
    info( autosaveFields_pass0, "VAL")
}

record(dfanout, "$(P)Omsl:Cont-FOut0_") {
    field( OMSL, "closed_loop")
    field( DOL,  "$(P)Omsl-FOut")
    field( OUTA, "$(P)Src:Mxc6-Sel.OMSL")
    field( OUTB, "$(P)Src:Mxc7-Sel.OMSL")
    field( OUTC, "$(P)Src:RearInp0-Sel.OMSL")
    field( OUTD, "$(P)Src:RearInp1-Sel.OMSL")
    field( OUTE, "$(P)Src:RearInp2-Sel.OMSL")
