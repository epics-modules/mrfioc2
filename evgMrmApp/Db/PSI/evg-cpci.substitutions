file evgMrm.db {
pattern { SYS,      EVG}
        {"$(SYS)", "$(EVG)"}
}

file evgSoftEvt.db {
pattern { SYS,      EVG,      OBJ }
        {"$(SYS)", "$(EVG)", "SoftEvt" }
}

file evgAcTrig.db {
pattern { SYS,      EVG,      OBJ }
        {"$(SYS)", "$(EVG)", "AcTrig" }
}

file evgEvtClk.db {
