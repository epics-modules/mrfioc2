        {"$(SYS)", "$(EVG)", "EvtClk" }
}

file evgDbus.db {
pattern { SYS,      EVG,      OBJ,    ID }
        {"$(SYS)", "$(EVG)", "Dbus",  0  }
        {"$(SYS)", "$(EVG)", "Dbus",  1  }
        {"$(SYS)", "$(EVG)", "Dbus",  2  }
        {"$(SYS)", "$(EVG)", "Dbus",  3  }
        {"$(SYS)", "$(EVG)", "Dbus",  4  }
        {"$(SYS)", "$(EVG)", "Dbus",  5  }
        {"$(SYS)", "$(EVG)", "Dbus",  6  }
        {"$(SYS)", "$(EVG)", "Dbus",  7  }

}

file evgInput.db {
pattern { SYS,      EVG,      OBJ,       ID,  PPS,  DBUSOBJ}
        {"$(SYS)", "$(EVG)", "UnivInp",  0,   3,    "Dbus"}
        {"$(SYS)", "$(EVG)", "UnivInp",  1,   4,    "Dbus"}
        {"$(SYS)", "$(EVG)", "UnivInp",  2,   5,    "Dbus"}
        {"$(SYS)", "$(EVG)", "UnivInp",  3,   6,    "Dbus"}
}

