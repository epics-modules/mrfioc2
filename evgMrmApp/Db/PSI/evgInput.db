record(bo, "$(SYS)-$(EVG):$(OBJ)$(ID)-EnaIrq-Sel") {
    field( DESC, "Enable 1 Pulse Per Second Input")
    field( DTYP, "Obj Prop bool")
    field( OUT , "@OBJ=$(EVG):$(OBJ)$(ID), PROP=IRQ")
    field( ZNAM, "Disabled")
    field( ONAM, "Enabled")
    field( FLNK, "$(SYS)-$(EVG):$(OBJ)$(ID)-EnaIrq-RB")
    field( VAL, "$($(OBJ)$(ID)-EnaIrq-Sel\=0)")
    info( autosaveFields_pass0, "VAL")
}

record(bi, "$(SYS)-$(EVG):$(OBJ)$(ID)-EnaIrq-RB") {
    field( DESC, "Enable External Input IRQ")
    field( DTYP, "Obj Prop bool")
    field( INP , "@OBJ=$(EVG):$(OBJ)$(ID), PROP=IRQ")
    field( ZNAM, "Disabled")
    field( ONAM, "Enabled")
}


