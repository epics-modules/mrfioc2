record(bo, "EVG$(cardNum):Mxc$(mxcNum)Polarity") {
	field( DTYP, "EVG Mxc")
  	field( DESC, "EVG Mux Counter Output Polarity")
  	field( OUT , "#C$(cardNum) S$(mxcNum) @")
  	field( PINI, "YES")
	field( VAL, "0")
	field( UDF, "0")
  	field( ZNAM, "Orginal")
  	field( ONAM, "Inverted")
}

record(bi, "EVG$(cardNum):Mxc$(mxcNum)Status") {
	field( DTYP, "EVG Mxc")
  	field( DESC, "EVG Mux Counter Output Status")
  	field( SCAN, "1 second")
	field( INP , "#C$(cardNum) S$(mxcNum) @")
	field( ZNAM, "0")
  	field( ONAM, "1")
}

record(longout , "EVG$(cardNum):Mxc$(mxcNum)Presacler") {
	field( DTYP, "EVG Mxc")
	field( DESC, "EVG Mux Counter Prescaler")
	field( OUT , "#C$(cardNum) S$(mxcNum) @")
	field( PINI, "YES")
	field( VAL , "12500")
	field( UDF , "0")
}

